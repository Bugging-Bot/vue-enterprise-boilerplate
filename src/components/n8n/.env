# env file for n8n
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
# Database Configuration
DB_TYPE=sqlite
# DB_TYPE=postgresdb
# DB_POSTGRESDB_HOST=localhost
# DB_POSTGRESDB_PORT=5432
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=n8n
# DB_POSTGRESDB_PASSWORD=n8n
# Webhooks URL (important for external access)
WEBHOOK_URL=http://localhost:5678/
# N8N_RUNNERS_ENABLED (as per your previous query)
N8N_RUNNERS_ENABLED=true

# Enable Basic Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=my-user@n8n.com
N8N_BASIC_AUTH_PASSWORD=Secure-Password@123

# Force n8n to use basic authentication for the editor UI
N8N_AUTH_MODE=basic

# Other settings like SMTP, S3 for credentials, etc.
# N8N_SMTP_HOST=smtp.example.com
# N8N_SMTP_PORT=587
# N8N_SMTP_USER=user@example.com
# N8N_SMTP_PASS=your_smtp_password
# N8N_SMTP_SENDER=n8n@example.com
